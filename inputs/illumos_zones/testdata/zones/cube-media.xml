<!DOCTYPE zone PUBLIC "-//Sun Microsystems Inc//DTD Zones//EN" "file:///usr/share/lib/xml/dtd/zonecfg.dtd.1">
<zone name="cube-media" zonepath="/zones/cube-media" autoboot="true" brand="lipkg" ip-type="exclusive" debugid="128">
  <filesystem special="/export/home" directory="/home" type="lofs"/>
  <filesystem special="/export/flac" directory="/storage/flac" type="lofs">
    <fsoption name="ro"/>
  </filesystem>
  <filesystem special="/export/mp3" directory="/storage/mp3" type="lofs">
    <fsoption name="ro"/>
  </filesystem>
  <attr name="dns-domain" type="string" value="localnet"/>
  <attr name="resolvers" type="string" value="192.168.1.26,192.168.1.1"/>
  <network allowed-address="192.168.1.25/24" physical="media_net0" defrouter="192.168.1.1" mac-addr="2:8:20:1:39:4e" global-nic="auto"/>
</zone>
